//-Lists
div.row
	div.col-sm-4
		h3.guide Unordered list
		ul
			-var i = 5; while(i--)
				li Item #{5-i}

	div.col-sm-4
		h3.guide Ordered list
		ol
			-var i = 5; while(i--)
				li Item #{5-i}

	div.col-sm-4
		h3.guide Definition list
		dl
			-var i = 5; while(i--)
				dt Term #{5-i}
				dd Definition #{5-i}
